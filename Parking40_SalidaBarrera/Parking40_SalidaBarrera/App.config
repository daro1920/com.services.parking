<?xml version="1.0"?>
<configuration>

  <appSettings>
    <add key="TOLERANCIA_SALIDA" value="10"/> <!--Tolerancia de tiempo para salida en minutos-->
    <add key="PORT_O" value="COM1"/> <!--Puerto de salida a controladora de barrera-->
    <add key="ENVIAR_SALIDA" value="NO"/> <!--Enviar senal de salida por PORT_O-->
    <add key="DEMORA_SENAL_SALIDA" value="500"/> <!--Demora de la senal de salida en ms-->
    <add key="GUARDAR_EN_LOG" value="SI"/> <!--Guardar eventos del sistema en archivo de log-->
    <add key="ARCHIVO_LOG" value="PARKING_40_LOG.TXT"/> <!--Nombre (y path) de archivo de logeo-->
    <add key="NRO_BARRERA" value="1"/> <!--Nro de barrera controlada-->
    <add key="MOSTRAR_MENSAJES" value="SI"/> <!--Mostrar mensajes para tickets no validos-->
    <add key="TIMEOUT_MENSAJES" value="2000"/> <!--Timeout de mensajes para tickets no validos (en ms)-->
    <add key="BEEP_SOUND_SUCCES" value="C:\Work\FreelanceProjects\parking_img_sounds\beeps\beep-succes.wav"/> <!--Reproduccion de sonidos exito-->
	  <add key="BEEP_SOUND_CARD" value="C:\Work\FreelanceProjects\parking_img_sounds\beeps\beep-tarjeta.wav"/> <!--Reproduccion de sonidos exito-->
	  <add key="BEEP_SOUND_NOT_ALLOW" value="C:\Work\FreelanceProjects\parking_img_sounds\beeps\beep-no_habilitado.wav"/> <!--Reproduccion de sonidos exito-->
	  <add key="BEEP_SOUND_TIME_OUT" value="C:\Work\FreelanceProjects\parking_img_sounds\beeps\beep-exedido.wav"/> <!--Reproduccion de sonidos exito-->
    <add key="TICKET_IMAGE" value="C:\image\ticket_parking.jpg"/>
    <add key="TICKET_ROWS" value="C:\image\flecha.jpg"/>
  </appSettings>

  <connectionStrings>
    <remove name="PARKING"/>
    <add name="PARKING" connectionString="Server=DaroGimeVaio\LOCALHOST; Trusted_Connection=Yes;"/>
  </connectionStrings>

  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>

</configuration>
